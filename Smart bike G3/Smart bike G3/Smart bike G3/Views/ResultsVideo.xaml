<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Smart_bike_G3.Views.ResultsVideo"
             BackgroundColor="White">
    <ContentPage.Content>
        <StackLayout Margin="48">
            <Label FontFamily="Rubik-Bold" Margin="16" Text="Video" FontSize="48" FontAttributes="Bold" HorizontalTextAlignment="Center"  TextColor="#302e33"/>
            <Label FontFamily="Rubik-SemiBold" Text="Resultaat" FontSize="40" FontAttributes="Bold" HorizontalTextAlignment="Center"  TextColor="#615C66"/>
            <Label FontFamily="Rubik-Regular" x:Name="lblKilometers"  Margin="88" Text="12 km" FontSize="40" HorizontalTextAlignment="Center"  TextColor="#302e33"/>
            <Button  FontFamily="Rubik-Regular" Margin="88" x:Name="btnScorebord" Text="Scorebord" TextColor="White" BackgroundColor="#5589ee" CornerRadius="10"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>
