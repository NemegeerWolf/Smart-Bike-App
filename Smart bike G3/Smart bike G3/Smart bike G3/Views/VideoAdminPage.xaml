<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Smart_bike_G3.Views.VideoAdminPage">
    <ContentPage.Content>
        <StackLayout>
            <Entry x:Name="video1"/>
            <RadioButton x:Name="radio10" GroupName="1" Content="Video Audio" IsChecked="True"/>
            <RadioButton x:Name="radio11" GroupName="1" Content="Muziek"/>
            <Entry x:Name="video2"/>
            <RadioButton x:Name="radio20" GroupName="2" Content="Video Audio" IsChecked="True"/>
            <RadioButton x:Name="radio21" GroupName="2" Content="Muziek" />
            <Entry x:Name="video3"/>
            <RadioButton x:Name="radio30" GroupName="3" Content="Video Audio" IsChecked="True"/>
            <RadioButton x:Name="radio31" GroupName="3" Content="Muziek"/>
            <Entry x:Name="video4"/>
            <RadioButton x:Name="radio40" GroupName="4" Content="Video Audio" IsChecked="True"/>
            <RadioButton x:Name="radio41" GroupName="4" Content="Muziek"/>
            <Button x:Name="changeBtn" Clicked="ChangeBtn_Clicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>