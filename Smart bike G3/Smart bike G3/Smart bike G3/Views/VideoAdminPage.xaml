<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Smart_bike_G3.Views.VideoAdminPage">
    <ContentPage.Content>
        <StackLayout>
            <Entry x:Name="video1"/>
            <RadioButton x:Name="radio1" GroupName="1" Content="Video Audio" IsChecked="True"/>
            <RadioButton  GroupName="1" Content="Muziek"/>
            <Entry x:Name="video2"/>
            <RadioButton x:Name="radio2" GroupName="2" Content="Video Audio" IsChecked="True"/>
            <RadioButton  GroupName="2" Content="Muziek" />
            <Entry x:Name="video3"/>
            <RadioButton x:Name="radio3" GroupName="3" Content="Video Audio" IsChecked="True"/>
            <RadioButton GroupName="3" Content="Muziek"/>
            <Entry x:Name="video4"/>
            <RadioButton x:Name="radio4" GroupName="4" Content="Video Audio" IsChecked="True"/>
            <RadioButton  GroupName="4" Content="Muziek"/>
            <Button x:Name="changeBtn" Clicked="changeBtn_Clicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>